{
    "type": 1,
    "title": "preinstallpfc",
    "logo": "https://github.com/Helmut-Saal/docker-templates/blob/main/icons/wagoOpenSource.png?raw=true",
    "description": "Preinstall PFC with IP",
    "image": "script/con:1.0.1",
    "restart_policy": "no",
    "name": "preinstall",
    "command": "/home/mein.sh";
    "platform": "linux",
    "env": [
      {
        "name": "PFC_IP",
        "label": "IP Address of the PFC",
        "default": "192.168.10.30"    
      },
	  {
        "name": "PFC_PASSWORD",
        "label": "New Pfc Password",
        "default": "192.168.10.100"    
      },
	  {
        "name": "PFC_HOSTNAME",
        "label": "New Hostname",
        "default": "PFC_XX"    
      }
    ]
  }
