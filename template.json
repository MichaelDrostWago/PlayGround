{
"version": "2",
"templates":
[
  {
    "type": 1,
    "title": "preinstall pfc",
    "logo": "https://github.com/Helmut-Saal/docker-templates/blob/main/icons/swarm.png?raw=true",
    "description": "preset pfc",
    "image": "script/con:1.0.1",
    "name": "preinstall-pfc",
    "restart_policy": "no",
    "command": "/home/mein.sh",
    "platform": "linux",
    "volumes": [
      {
        "bind": "/tmp",
        "container": "/tmp"
      }
    ],
	"env": [
      {
        "name": "WORKER_IP",
        "label": "IP Adress of new worker node.",
        "default": "192.168.10.30"    
      },
	  {
        "name": "MANAGER_IP",
        "label": "IP Adress of the manager node.",
        "default": "192.168.10.100"    
      },
	  {
        "name": "CA_PATH",
        "label": "Folder of the ca file.",
        "default": "/home/provisionPLC/"    
      }
    ]
  }
]
}
